<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/bg">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView10"
        android:src="@drawable/profile"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="35dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/name"
        android:layout_alignTop="@+id/imageView10"
        android:layout_toRightOf="@+id/imageView10"
        android:layout_marginLeft="15dp"
        android:lines="2"
        android:maxWidth="150dp"
        android:maxLines="2"
        android:minLines="2"
        android:layout_marginTop="2dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/cidar"
        android:layout_marginRight="21dp"
        android:layout_marginTop="14dp"
        android:layout_alignTop="@+id/imageView10"
        android:layout_alignRight="@+id/scrollView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Curso:"
        android:id="@+id/textView10"
        android:layout_alignTop="@+id/imageView10"
        android:layout_toLeftOf="@+id/cidar"
        android:layout_marginRight="20dp"
        android:textSize="14dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/prldr"
        android:layout_alignTop="@+id/ci"
        android:layout_toRightOf="@+id/ci" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/ci"
        android:layout_alignTop="@+id/cidar"
        android:layout_marginLeft="5dp"
        android:layout_alignLeft="@+id/textView10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/usr"
        android:layout_alignTop="@+id/imageView10"
        android:layout_marginTop="55dp"
        android:layout_alignLeft="@+id/name" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView"
        android:layout_gravity="center"
        android:layout_marginBottom="15dp"
        android:background="#90000000"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp">

        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:minHeight="340dp">

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="left|top"
                android:background="#95FFFFFF"
                android:padding="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="15dp"
                android:onClick="news">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/news" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Noticias"
                    android:id="@+id/textView"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="15dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_gravity="center_horizontal|top"
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:background="#95FFFFFF"
                android:padding="10dp"
                android:layout_marginTop="15dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView2"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/callendar" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Cronograma"
                    android:id="@+id/textView2"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="10dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="right|top"
                android:padding="10dp"
                android:background="#95FFFFFF"
                android:layout_marginTop="15dp"
                android:layout_marginRight="15dp"
                android:onClick="profile"
                android:clickable="true">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView3"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/profile" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Perfil"
                    android:id="@+id/textView3"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="15dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:padding="10dp"
                android:layout_gravity="center_vertical|left"
                android:background="#95FFFFFF"
                android:layout_marginLeft="15dp"
                android:onClick="rankac"
                android:clickable="true">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView4"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/ranking" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Ranking del curso"
                    android:id="@+id/textView4"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="10dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="center"
                android:background="#95FFFFFF"
                android:padding="10dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView5"
                    android:layout_gravity="center_horizontal|top" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Calcula mi quimestre"
                    android:id="@+id/textView5"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="10dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="center_vertical|right"
                android:background="#95FFFFFF"
                android:padding="10dp"
                android:layout_marginRight="15dp"
                android:onClick="readcm">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Notas"
                    android:id="@+id/textView6"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="15dp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView6"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/notas" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="bottom|left"
                android:background="#95FFFFFF"
                android:padding="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_marginBottom="15dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView7"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/location" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="UbicaciÃ³n"
                    android:id="@+id/textView7"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="13dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal|bottom"
                android:background="#95FFFFFF"
                android:padding="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="15dp">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView8"
                    android:layout_gravity="center_horizontal|top"
                    android:clickable="false"
                    android:src="@drawable/logo_firma" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Nuestro compromiso"
                    android:id="@+id/textView8"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="10dp" />
            </FrameLayout>

            <FrameLayout
                android:layout_width="80dp"
                android:layout_height="90dp"
                android:layout_gravity="bottom|right"
                android:background="#95FFFFFF"
                android:padding="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="15dp"
                android:layout_marginBottom="15dp"
                android:onClick="about">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/imageView9"
                    android:layout_gravity="center_horizontal|top"
                    android:src="@drawable/about" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Acerca de"
                    android:id="@+id/textView9"
                    android:layout_gravity="center_horizontal|bottom"
                    android:textSize="13dp" />
            </FrameLayout>

        </FrameLayout>
    </ScrollView>

</RelativeLayout>